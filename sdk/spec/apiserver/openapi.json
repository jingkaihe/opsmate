{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/v1/health": {
      "get": {
        "summary": "Health",
        "operationId": "health_v1_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Health"
                }
              }
            }
          }
        }
      }
    },
    "/v1/models": {
      "get": {
        "summary": "Models",
        "operationId": "models_v1_models_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Model"
                  },
                  "type": "array",
                  "title": "Response Models V1 Models Get"
                }
              }
            }
          }
        }
      }
    },
    "/v1/run": {
      "post": {
        "summary": "Run",
        "operationId": "run_v1_run_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RunRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "type": "object"
                  },
                  "type": "array",
                  "title": "Response Run V1 Run Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Health": {
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "ok",
              "faulty"
            ],
            "title": "status",
            "default": "ok"
          }
        },
        "type": "object",
        "title": "Health"
      },
      "Model": {
        "properties": {
          "provider": {
            "type": "string",
            "title": "provider"
          },
          "model": {
            "type": "string",
            "title": "model"
          }
        },
        "type": "object",
        "required": [
          "provider",
          "model"
        ],
        "title": "Model"
      },
      "RunRequest": {
        "properties": {
          "model": {
            "type": "string",
            "title": "name of the llm model to use"
          },
          "provider": {
            "type": "string",
            "title": "name of the provider to use"
          },
          "instruction": {
            "type": "string",
            "title": "instruction to execute"
          },
          "contexts": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "contexts to use",
            "default": [
              "cli"
            ]
          },
          "ask": {
            "type": "boolean",
            "title": "ask",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "model",
          "provider",
          "instruction"
        ],
        "title": "RunRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}